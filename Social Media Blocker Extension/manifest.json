{
  "name": "Social Media Blocker",
  "manifest_version": 2,
  "description": "Welcome here",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["data.js"]
    }
  ],
  "icons": { 
    "128": "logoo.png"
   },
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "Social Media Blocker"
  },
  "permissions": ["activeTab"]
  
}
